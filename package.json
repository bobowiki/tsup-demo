{
  "name": "tsup-pkg",
  "version": "1.0.0",
  "author": "bobowiki",
  "main": "./dist/index.js",
  "packageManager": "pnpm@9.1.4",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "build": "tsup",
    "build:watch": "tsup --watch"
  },
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.4.0",
    "globals": "^15.3.0",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0"
  },
  "dependencies": {},
  "simple-git-hooks": {
    "pre-commit": "pnpm link-staged"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  }
}
